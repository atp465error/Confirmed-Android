<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <solid
                android:color="#fff"/>
            <padding
                android:bottom="20dp"
                android:left="20dp"
                android:right="20dp"
                android:top="20dp"/>
            <stroke
                android:width="1dp"
                android:color="#FFF"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval">
            <solid
                android:color="#fff"/>

            <size
                android:width="200dp"
                android:height="200dp"/>
            <stroke
                android:width="5dp"
                android:color="#BBB"/>
        </shape>
    </item>
    <item>
    <inset android:drawable="@drawable/tt_power_button_img"
            android:insetRight="25dp"
            android:insetTop="25dp"
            android:insetLeft="25dp"
            android:insetBottom="25dp" />
    </item>
</layer-list>